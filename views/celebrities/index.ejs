<h2>Search Celebrity</h2>
<label>Name</label>
<form method="get" action="/celebrities">
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<br>
<% celebrities.forEach(celebrity => { %>
    <div>
        <%= celebrity.name %>
    </div>
    <a href="/celebrities/<%= celebrity.id %> ">View</a>
    <a href="/celebrities/<%= celebrity.id %>/edit">Edit</a>
    <%- include('../partials/deleteform', { url: `/celebrities/${celebrity.id}` }) %> 
    <% }) %>